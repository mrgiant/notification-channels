{
    "name": "mrgiant/notification-channels",
    "description": "A collection of custom notification providers, offering seamless integration with various channels to enhance your application's messaging capabilities.",
    "type": "library",
    "license": "MIT",

    "keywords": [
        "notifications",
        "notification-channels",
        "laravel"
    ],
    "authors": [
        {
            "name": "Mohamed Al Wahaibi",
            "email": "mohamed.alwahaibi@golden-logic.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/spatie/laravel-medialibrary",

    "require": {
        "php": "^8.1",
        

        "illuminate/support/facades/http": "^8.0 || ^9.0 || ^10.0 || ^11.0",
    },

    "require-dev": {
        
        "guzzlehttp/guzzle": "^7.8.1"
    },





    "minimum-stability": "dev",
    "prefer-stable": true,

    "autoload": {
        "psr-4": {
            "Mrgiant\\NotificationChannels\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrgiant\\NotificationChannels\\NotificationChannelsServiceProvider"
            ]
        }
    }
   

}
